{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "0.1.0",
  "default_locale": "ja",
  "description": "__MSG_extDescription__",
  "author": "Yuichi Tanikawa",
  "homepage_url": "https://github.com/itiut/ocg-card-name-link",
  "icons": {
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "exclude_matches": [
        "http://yugioh-wiki.net/*"
      ],
      "js": ["js/main.js"]
    }
  ],
  "permissions": [
  ],
  "optional_permissions": [
    "<all_urls>"
  ],
  "options_ui": {
    "page": "html/options.html",
    "chrome_style": true
  },
  "minimum_chrome_version": "53"
}
